<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Insurance Dashboard</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>

    <!-- Header Section -->
    <header>
        <div class="logo">
            <img src="{{imageSrc}}" alt="Kaito Logo">
        </div>
        <div class="company-info">
            <h1>Kaito Assurance Brokers Private Limited</h1>
            <p>Chennai, Tamil Nadu</p>
        </div>
    </header>

    <!-- Request Quote Button -->
    <div class="quote-section">
        <button class="quote-btn">Request for quote - GMC</button>
    </div>

    <!-- Client Information -->
    {{> client-info }}

    <!-- Product Information -->
    {{> product-info }}    

    <!-- Lives Section -->
    {{> lives-info }}     

<div class="page-break"></div>
    <!-- Expiring Premium -->
    {{> expiring-premium }}
    
    <!-- claim information -->
    {{> claim-information }}

    <!-- coverages info -->
    {{> coverages }}   
   
    <!-- premium info -->
    {{> premium }} 

    <!-- documents uploaded -->
    {{> documents }}    
